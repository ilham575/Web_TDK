{
  "kind": "collectionType",
  "collectionName": "uploaded_documents",
  "info": {
    "singularName": "uploaded-document",
    "pluralName": "uploaded-documents",
    "displayName": "uploaded_document"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "file_url": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "submitted_by": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "uploaded_documents"
    },
    "uploaded_at": {
      "type": "datetime",
      "required": true,
      "unique": false
    }
  }
}
