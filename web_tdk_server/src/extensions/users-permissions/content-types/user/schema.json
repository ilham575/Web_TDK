{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "teacher": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::teacher.teacher",
      "mappedBy": "user"
    },
    "parent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::parent.parent",
      "mappedBy": "user"
    },
    "student": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::student.student",
      "inversedBy": "user"
    },
    "score_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::score-category.score-category",
      "mappedBy": "created_user"
    },
    "announcements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::announcement.announcement",
      "mappedBy": "posted_by"
    },
    "uploaded_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::uploaded-document.uploaded-document",
      "mappedBy": "submitted_by"
    }
  }
}
